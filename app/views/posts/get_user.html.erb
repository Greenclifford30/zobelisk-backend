<h1>Listing User Info</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Email</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>DoB</th>
      <th>Address</th>
      <th>Phone Number</th>
      <th>Twitter</th>
      <th>Picture</th>
    </tr>
  </thead>

  <tbody>
    <% @user.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.dob %></td>
        <td><%= user.address %></td>
        <td><%= user.phone %></td>
        <td><%= user.twitter %></td>
        <td><%= user.picture %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New user', new_user_path %>
